{
    "extends": [
    ],
    "parserOptions": {
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "globals": {
        "jsRoutes": true,
        "$": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
        "no-alert": "warn",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "react/jsx-no-undef": "error"
    }
}
